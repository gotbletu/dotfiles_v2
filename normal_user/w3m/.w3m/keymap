# Bookmark Management {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  156:<NOT       View bookmarks (BOOKMARK)
#  157:ASSIGNED>
#  158:<NOT       Add current page to bookmarks (ADD_BOOKMARK)
#  159:ASSIGNED>
# }}}
# Searches {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  163:<NOT       Search forward (WHEREIS)
#  164:ASSIGNED>
#  165:<NOT       Search backward (SEARCH_BACK)
#  166:ASSIGNED>
keymap  n     SEARCH_NEXT
keymap  N     SEARCH_PREV
keymap  /     ISEARCH
keymap  ?     ISEARCH_BACK
# }}}
# Dictionary Lookup {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  174:<NOT       Execute dictionary command (see README.dict) (DICT_WORD)
#  175:ASSIGNED>
#  176:<NOT       Execute dictionary command for word at cursor (DICT_WORD_AT)
#  177:ASSIGNED>
# }}}
# Mark Operations {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  181:<NOT       Set/unset mark (MARK)
#  182:ASSIGNED>
#  183:<NOT       Go to the next mark (NEXT_MARK)
#  184:ASSIGNED>
#  185:<NOT       Go to the previous mark (PREV_MARK)
#  186:ASSIGNED>
#  187:<NOT       Mark all occurences of a pattern (REG_MARK)
#  188:ASSIGNED>
# }}}
# Miscellaneous {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
keymap  gM      MAIN_MENU
#  192:<NOT       Pop up menu (MAIN_MENU)
#  193:ASSIGNED>
keymap  :H      HELP
keymap	:O      OPTIONS
keymap	:V      VERSION
#  196:<NOT       Display the version of w3m (VERSION)
#  197:ASSIGNED>
#  198:<NOT       View cookie list (COOKIE)
#  199:ASSIGNED>
keymap  gH      HISTORY
keymap  :h      HISTORY
keymap  gd      DOWNLOAD_LIST
#  202:ASSIGNED>
#  203:<NOT       Change the character encoding for the current document (CHARSET)
#  204:ASSIGNED>
#  205:<NOT       Change the default character encoding (DEFAULT_CHARSET)
#  206:ASSIGNED>
#  207:<NOT       Display error messages (MSGS)
#  208:ASSIGNED>
keymap	m	MOUSE_TOGGLE
keymap	W	WRAP_TOGGLE
#  210:<NOT       Toggle wrapping mode in searches (WRAP_TOGGLE)
#  211:ASSIGNED>
keymap  ::      COMMAND
#  213:<NOT       Set alarm (ALARM)
#  214:ASSIGNED>
#  215:<NOT       Set option (SET_OPTION)
#  216:ASSIGNED>
#  217:<NOT       Set environment variable (SETENV)
#  218:ASSIGNED>
#  219:<NOT       Define a binding between a key stroke combination and a command (DEFINE_KEY)
#  220:ASSIGNED>
#  221:<NOT       Reload configuration file (REINIT)
#  222:ASSIGNED>
#  223:!          Execute shell command and display output (SHELL)
keymap	!     EXEC_SHELL
#  224:C-z        Suspend w3m to background (INTERRUPT)
keymap  :q    QUIT
keymap  ZZ    QUIT
keymap  ZQ    EXIT
keymap	Q     EXIT
# }}}
# User-defined key bindings {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# keymap  :B      SHELL ~/.scripts/w3m_scripts_collection/w3m_copy_bookmarks_to_clipboard_via_fzf.sh
# keymap  :b      SHELL ~/.scripts/w3m_scripts_collection/w3m_copy_bookmarks_to_tmux_clipboard_via_fzf.sh
keymap  :b       SHELL "$EDITOR ~/.config/surfraw/bookmarks"
keymap  :c       SHELL "$EDITOR ~/.w3m/config"
keymap  :k       SHELL "$EDITOR ~/.w3m/keymap"
keymap  :m       SHELL "$EDITOR ~/.w3m/mailcap"
keymap  :u       SHELL "$EDITOR ~/.w3m/urimethodmap"
keymap  :t       SHELL "$EDITOR ~/.w3m/RestoreTab.txt"

# }}}
# Buffer and Tab Selection Mode {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  236:
#  237:C-n,j      Next item
#  238:k,C-p      Previous item
#  239:D          Delete item
#  240:RET        Select item
# }}}
# Input Line Editing Mode {{{
# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
#  245:
#  246:C-f        Cursor forward
#  247:C-b        Cursor backward
#  248:C-h        Delete previous character
#  249:C-d        Delete character
#  250:C-k        Delete everything after cursor
#  251:C-u        Delete everything before cursor
#  252:C-a        Cursor to the beginning of the line
#  253:C-e        Cursor to the end of the line
#  254:C-p        Fetch the previous string from the history list
#  255:C-n        Fetch the next string from the history list
#  256:C-o        Edit with external editor
#  257:SPC,TAB    Try to complete filename
#  258:RET        Accept input line
# }}}
# Popup Menu Mode {{{
#  264:SPC,RET,RIGHT  Select item
#  265:^[[2~,^        Close menu
#  266:[[28~,C-c
#  267:LEFT,C-h,DEL   One selection step backward
#  268:j,C-n,DOWN     Move to the next item
#  269:k,UP,C-p       Move to the previous item
#  270:K              Scroll up one item
#  271:J              Scroll down one item
#  272:C-a            Move to the first item
#  273:C-e            Move to the final item
#  274:^[[6~,C-f,C-v  Go to the next page
#  275:^              Go to the previous page
#  276:[[5~,ESC-v,C-b
#  277:C-s,/          Search forward
#  278:?,C-r          Search backward
#  279:n              Search for the next match
#  280:N              Search for the previous match
#  281:C-z            Suspend
# }}}

# }}}
# External Commands {{{
# EXTERN_LINK = under cursor
# EXTERN      = current page

# yank url to clipboard
# keymap  yx      EXTERN_LINK   '(echo -n %s | xsel -b)'
keymap  yx      EXTERN_LINK   '(printf %s | xsel -b)'
keymap  YX      EXTERN        '(printf %s | xsel -b)'
# yank url to tmux clipboard
keymap  yy      EXTERN_LINK   'tmux set-buffer'
keymap  YY      EXTERN        'tmux set-buffer'

# open gui browser
keymap  xw      EXTERN_LINK   '$BROWSER'
keymap  XW      EXTERN        '$BROWSER'
keymap  xn      EXTERN_LINK   '$BROWSER_PRIVATE'
keymap  XN      EXTERN        '$BROWSER_PRIVATE'

# queue download file (task-spooler)
# keymap  xd      EXTERN_LINK   'TS_SOCKET=/tmp/w3m tsp aria2c -j 1 -x 2 -c -d ~/Downloads'
keymap  yd      EXTERN_LINK   'youtube-dl -o "~/Downloads/%(title)s.%(ext)s"'
keymap  xd      EXTERN_LINK   'diana add'
keymap  XD      EXTERN_LINK   'diana --pause add'
keymap  xD      SHELL         'diana-mui'

# keymap  wg      EXTERN_LINK   'TS_SOCKET=/tmp/w3m tsp wget -c -P /media/EdoTensei/Wget'
# keymap  WG      EXTERN        'TS_SOCKET=/tmp/w3m tsp wget -c -P /media/EdoTensei/Wget'
# keymap  wG      SHELL         'watch TS_SOCKET=/tmp/w3m'
# keymap  wc      SHELL         'TS_SOCKET=/tmp/w3m -C'

# queue external media player (task-spooler) e.g youtube, video links
keymap  xm      EXTERN_LINK   'tsp mpv --ontop --no-border --force-window --autofit=500x280 --geometry=-15-53'
keymap  XM      EXTERN        'tsp mpv --ontop --no-border --force-window --autofit=500x280 --geometry=-15-53'

# open external image viewer
# keymap  xi      EXTERN_LINK   'w3m -o display_image=1 -o imgdisplay=w3mimgdisplay'
keymap  xf      EXTERN_LINK   'feh -. -x -B black -g 900x600-15+60 %s &'
keymap  xF      EXTERN_LINK   'mpv --loop --quiet --ontop --no-border --force-window --autofit=900x600 --geometry=-15+60'

# add torrent or magnetlinks
# keymap  xt      EXTERN_LINK   'transmission-remote --add'
keymap  xt      EXTERN_LINK   ~/.scripts/privatescripts/addtorrent-thinkcentre
keymap  XT      EXTERN        ~/.sripts/shellscripts/torrentportal

# basic task-spooler view, cat, clear
# keymap  xs      SHELL         'watch TS_SOCKET=/tmp/w3m tsp'
# keymap  XS      SHELL         'clear && TS_SOCKET=/tmp/w3m tsp -c'
# keymap  xc      SHELL         'clear && TS_SOCKET=/tmp/w3m tsp -C'

# keymap  xc      SHELL         'aria2c $W3M_URL'
# keymap  xc      SHELL         'aria2c $W3M_CURRENT_LINK'
# keymap  xc      SHELL         'aria2c $W3M_CURRENT_IMG'
keymap  xv      SHELL         'vim $W3M_CURRENT_FORM'

# W3M_CHARSET=UTF-8
# W3M_CURRENT_COLUMN=14
# W3M_CURRENT_FORM=
# W3M_CURRENT_IMG=
# W3M_CURRENT_LINE=14
# W3M_CURRENT_LINK=https://www.youtube.com/watch?v=n3Nw8MjXWgQ
# W3M_CURRENT_WORD=Peertube
# W3M_FILENAME=/
